class animal:
  def make_sound(self):
    print("make ageneral sounds")
class Dog(animal):
  def make_sound(self):
    print("dog says:bow! bow!")
class Cat(animal):
  def make_sound(self):
    print("cat says:meow! meow!")
class Bird(animal):
  def make_sound(self):
    print("bird says:twety! twety!")
animals=[dog(), cat(), bird()]

for animal in animals:
  animal.make_sound()
